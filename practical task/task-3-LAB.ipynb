   "outputs": [],
   "source": [
    "import cv2\n",
    "img= cv2.imread('image.jpeg')\n",
    "img = cv2.cvtColor(img, cv2.COLOR_BGR2LAB)\n",
    "cv2.imshow('LAB image', img) \n",
    "cv2.waitKey(0)      \n",
    "cv2.destroyAllWindows()"
   ]
  }
